import { ApiProperty } from '@nestjs/swagger';
import { <%= className %>Dto } from './<%= moduleName %>.dto';
<% joins.forEach(name => { -%>
import { <%= pascalCase(name) %>Dto } from 'src/modules/<%= name %>/dto/<%= name %>.dto';
<% }); -%>

export class <%= className %>ReportDto extends <%= className %>Dto {
<% joins.forEach(name => { -%>
    @ApiProperty({ type: <%= pascalCase(name) %>Dto })
    <%= name %>: <%= pascalCase(name) %>Dto;
<% }); -%>
}
